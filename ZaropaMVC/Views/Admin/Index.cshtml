@model PagedList.IPagedList<ZaropaMVC.Models.ExpandedUserDTO>
@using PagedList.Mvc

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create",null, new { @class = "zaropa-a-override" })
</p>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LockoutEndDateUtc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccessFailedCount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.UserName }, new { @class = "zaropa-a-override" }) |
                @Html.ActionLink("Details", "Details", new { id = item.UserName }, new { @class = "zaropa-a-override" }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.UserName }, new { @class = "zaropa-a-override" })
            </td>
        </tr>
    }

</table>

